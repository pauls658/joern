match (first)-[df_edge:REACHES]->(bb)-[:PARENT_OF*1..]->(call)-[:PARENT_OF]->(args),path=(args)-[:PARENT_OF]->(arg)-[:PARENT_OF*1..]->(name) where call.type = "AST_CALL" and args.type = "AST_ARG_LIST" and name.type = "string" and name.code = df_edge.var and not any(n in nodes(path) where n.type = "AST_CALL") create (first)-[new:REACHES]->(arg) set new.var = name.code delete df_edge;
